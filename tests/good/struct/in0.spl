class List {
    int head;
    List tail;

    constr (int n, List l)
    {
        head = n;
        tail = l;
    }
}

List reverse(List l)
{
    List acc = null;

    while (l != null)
    {
        acc = new List(l.head, acc);
        l = l.tail;
    }

    return acc;
}

void printListInt(List l)
{
    while (l != null)
    {
        printInt(l.head);
        l = l.tail;
    }

    return;
}

List listFromArray(int[] arr)
{
    List acc = null;
    for (i = arr.length - 1 down to -1)
        acc = new List(arr[i], acc);
    return acc;
}

int main()
{
    int n = 10;
    int[] arr = new int[n];
    for (i = 0 to n)
        arr[i] = i;

    List l = listFromArray(arr);

    printListInt(l);

    List l2 = reverse(l);

    printListInt(l2);

    return 0;
}
