int main()
{
    int n = 2;
    while (n < 20)
    {
        int i = 2;
        bool isPrime = true;
        while (i * i <= n)
        {
            if (n % i == 0)
            isPrime = false;
            i++;
        }

        if (isPrime)
            printInt(n);

        n++;
    }

    return 0;
}
