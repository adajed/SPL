int(int) iter(int(int) f, int n)
{
    int(int) acc = \int x -> return x; ;
    int i = 0;
    while (i < n)
    {
        acc = \int x -> { return f(acc(x)); } ;
        i++;
    }

    return acc;
}

int main()
{
    int size = 10;
    int(int)[] fs = new int(int)[size];

    int i = 0;
    while (i < size)
    {
        fs[i] = iter(\int x -> return x + 1;, i);
        i++;
    }

    i = 0;
    while (i < size)
    {
        printInt(fs[i](0));
        i++;
    }

    return 0;
}
