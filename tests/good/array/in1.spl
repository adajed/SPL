int sum_array(int[] arr)
{
    int sum = 0;
    for (a in arr)
        sum = sum + a;
    return sum;
}

int max_array(int[] arr)
{
    int max = arr[0];
    for (a in arr)
        if (a > max)
            max = a;

    return max;
}

int min_array(int[] arr)
{
    int min = arr[0];
    for (a in arr)
        if (a < min)
            min = a;

    return min;
}

int main()
{
    int size = 10;
    int[] arr = new int[size];

    for (i = 0 to size)
        arr[i] = i;

    printInt(sum_array(arr));
    printInt(max_array(arr));
    printInt(min_array(arr));

    return 0;
}
